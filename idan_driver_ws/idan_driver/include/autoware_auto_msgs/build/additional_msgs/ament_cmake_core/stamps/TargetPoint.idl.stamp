#include "std_msgs/msg/Header.idl"
module additional_msgs {
  module msg {
    @verbatim (language="comment", text=
      " Target point for ml_controller")
    struct TargetPoint {
      std_msgs::msg::Header header;
      
      @default (value=0.0)
      float angle;

      @default (value=0.0)
      float distance;

      @default (value=0.0)
      float velocity;
    };
  };
};
